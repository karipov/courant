            "text": {
                "en": "<i>Language set to English</i>\n\nNow, choose how you'd like to set up your news:"
            },
            "markup": {
                "en": ["Manual", "Explore"]
            },
            "payload": ["set:2.1:manual", "set:2.2:explore"]
