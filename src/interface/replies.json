            "markup": {
                "en": ["✎ RSS", "✎ Channels", "General Settings"]
            },
            "payload": ["fin:3.1:rss", "fin:3.2:channels", "fin:3.3:general"]
        },
        "3.1": {
            "text": {
                "en": "<i>Modify RSS</i>\n\nHow would you like to modify your RSS feeds?"
            },
            "markup": {
                "en": ["« Back", "Delete"]
            },
            "payload": ["fin:3:back", "fin:3.1.1:delete"]
        },
        "3.1.1": {
            "text": {
                "en": "<i>Delete</i>\n\nPlease click on the RSS Feeds you'd like to unsubscribe from."
            },
            "markup": {
                "en": ["« Back"]
            },
            "payload": ["fin:3.1:back"]
        },
        "3.2": {
            "text": {
                "en": "<i>Modify Channels</i>\n\nHow would you like to modify your Telegram Channels?"
            },
            "markup": {
                "en": ["« Back", "Delete"]
            },
            "payload": ["fin:3:back", "fin:3.2.1:delete"]
