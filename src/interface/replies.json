            "markup": {
                "en": ["Done"]
            },
            "payload": ["set:3:done"]
        },

        "2.2": {
            "text": {
                "en": "<i>Explore</i>\n\nGreat! Send me a list of topics you'd like to see in your news feeds with a simple list of of words or phrases, each separated by a comma. For example:\n\n<code>US News, Astronomy, Telegram</code>"
            },
            "markup": {
                "en": ["« Back"]
            },
            "payload": ["set:2:back"]
        },
        "2.2.1": {
            "text": {
                "en": "Loading..."
            },
            "text2": {
                "en": "Suggestions for channels and RSS feeds to follow have been compiled. Select which ones you'd like to follow by clicking the buttons below:"
            }
        },


        "3": {
            "text": {
                "en": "<i>Menu</i>\n\n<code>{rss_num:02} feeds</code>\n<code>{channel_num:02} channels</code>\n<code>{time}</code>"
