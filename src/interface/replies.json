        "2.1": {
            "text": {
                "en": "<i>Manual Entry</i>\n\nGreat! Select the type of news feed you'd like to set up:"
            },
            "markup": {
                "en": ["RSS Feed", "Channels", "Go Back"]
            },
            "payload": ["fsm:2.1:rss", "fsm:2.1:channels", "fsm:2"]
        },
        "2.2": {
            "text": {
                "en": "<i>Explore</i>\n\nGreat! Send me a list of topics you'd like to see in your news feeds with a simple list of of words or phrases, each separated by a comma. For example:\n\n US News, Astronomy, Telegram"
            },
            "markup": {
                "en": ["Go Back"]
            },
            "payload": ["fsm:2"]
        }
    }
